{
  "name": "threequarters-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "prepare": "husky",
    "prettier": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "lint": "eslint",
    "postinstall": "fumadocs-mdx",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --log-level silent --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "lint-staged": "npx lint-staged",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit && echo",
    "test": "echo passed wow amazing"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css}": "prettier --write"
  },
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "fumadocs-core": "16.0.10",
    "fumadocs-mdx": "13.0.7",
    "fumadocs-ui": "16.0.10",
    "husky": "9.1.7",
    "lucide-react": "0.553.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rimraf": "6.1.0",
    "shadcn": "3.5.0",
    "tailwind-merge": "3.4.0",
    "tsx": "4.20.6",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@tailwindcss/postcss": "4.1.17",
    "@types/mdx": "2.0.13",
    "@types/node": "24.10.0",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.2",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "10.1.8",
    "lint-staged": "16.2.6",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3"
  },
  "pre-commit": "lint-staged",
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "lf",
    "printWidth": 80,
    "jsxSingleQuote": true,
    "importOrder": [
      "<BUILTIN_MODULES>",
      "",
      "^(react/(.*)$)|^(react$)",
      "",
      "<THIRD_PARTY_MODULES>",
      "^types$",
      "^@/env(.*)$",
      "",
      "^@/types/(.*)$",
      "^@/config/(.*)$",
      "^@/lib/(.*)$",
      "^@/hooks/(.*)$",
      "",
      "^@/components/ui/(.*)$",
      "^@/components/(.*)$",
      "",
      "^@/styles/(.*)$",
      "^@/app/(.*)$",
      "",
      "^[./]"
    ],
    "importOrderTypeScriptVersion": "5.3.2",
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "tailwindStylesheet": "./app/global.css"
  }
}
